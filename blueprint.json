{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"landingPage": "/wp-admin/edit.php?post_type=doc_page",
	"preferredVersions": {
		"wp": "6.4",
		"php": "7.4"
	},
	"steps": [
		{
			"step": "mkdir",
			"path": "/wordpress/wp-content/docs"
		},
		{
			"step": "login"
		},
		{
			"step": "unzip",
			"zipFile": {
				"resource": "url",
				"url": "https://github-proxy.com/partial/adamziel/playground-docs-workflow/html-pages"
			},
			"extractToPath": "/wordpress/wp-content/docs"
		},
		{
			"step": "defineWpConfigConsts",
			"consts": {
				"HTML_PAGES_PATH": "/wordpress/wp-content/docs"
			}
		},
		{
			"step": "installPlugin",
			"pluginZipFile": {
				"resource": "url",
				"url": "https://github-proxy.com/partial/adamziel/playground-docs-workflow/wp-docs-plugin"
			}
		}
	]
}
